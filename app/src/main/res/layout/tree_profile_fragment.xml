<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:background="@color/white"
        tools:context=".TreeProfileFragment">

        <TextView
            android:id="@+id/StatusTextView"
            android:layout_width="213dp"
            android:layout_height="33dp"
            android:background="@drawable/strokebhagwa1"
            android:paddingTop="5dp"
            android:text=""
            android:textAlignment="center"
            android:textColor="#ffffff"
            android:textSize="17dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/textView2"
            app:layout_constraintEnd_toEndOf="@+id/villageTextView"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/villageTextView"
            app:layout_constraintTop_toTopOf="@+id/textView2" />

        <ImageView
            android:id="@+id/treeProfileImageView"
            android:layout_width="123dp"
            android:layout_height="110dp"
            android:layout_marginTop="28dp"
            android:background="@drawable/stroke1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/harit_brij_logos_transparent_removebg_preview" />



        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="28dp"
            android:text="@string/UTID"
            android:textColor="#000000"
            android:textSize="24sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/treeProfileImageView" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="@string/District"
            android:textColor="#000000"
            android:textSize="24sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/textView7"
            app:layout_constraintTop_toBottomOf="@+id/textView7" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="@string/Block"
            android:textColor="#000000"
            android:textSize="24sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/textView8"
            app:layout_constraintTop_toBottomOf="@+id/textView8" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/Village"
            android:textColor="#000000"
            android:textSize="24sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/textView12"
            app:layout_constraintTop_toBottomOf="@+id/textView12" />

        <TextView
            android:id="@+id/utidTextView"
            android:layout_width="213dp"
            android:layout_height="33dp"
            android:layout_marginEnd="16dp"
            android:background="@drawable/strokebhagwa1"
            android:paddingTop="5dp"
            android:text=""
            android:textAlignment="center"
            android:textColor="#ffffff"
            android:textSize="17dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/textView7"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textView7"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/districtTextView"
            android:layout_width="213dp"
            android:layout_height="33dp"
            android:background="@drawable/strokebhagwa1"
            android:paddingTop="5dp"
            android:text=""
            android:textAlignment="center"
            android:textColor="#ffffff"
            android:textSize="17dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/textView8"
            app:layout_constraintEnd_toEndOf="@+id/utidTextView"
            app:layout_constraintStart_toStartOf="@+id/utidTextView"
            app:layout_constraintTop_toTopOf="@+id/textView8"
            app:layout_constraintVertical_bias="1.0" />

        <TextView
            android:id="@+id/blockTextView"
            android:layout_width="213dp"
            android:layout_height="33dp"
            android:background="@drawable/strokebhagwa1"
            android:paddingTop="5dp"
            android:text=""
            android:textAlignment="center"
            android:textColor="#ffffff"
            android:textSize="17dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/textView12"
            app:layout_constraintEnd_toEndOf="@+id/districtTextView"
            app:layout_constraintStart_toStartOf="@+id/districtTextView"
            app:layout_constraintTop_toTopOf="@+id/textView12"
            app:layout_constraintVertical_bias="1.0" />

        <TextView
            android:id="@+id/villageTextView"
            android:layout_width="213dp"
            android:layout_height="33dp"
            android:background="@drawable/strokebhagwa1"
            android:paddingTop="5dp"
            android:text=""
            android:textAlignment="center"
            android:textColor="#ffffff"
            android:textSize="17dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/textView15"
            app:layout_constraintEnd_toEndOf="@+id/blockTextView"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/blockTextView"
            app:layout_constraintTop_toTopOf="@+id/textView15"
            app:layout_constraintVertical_bias="1.0" />

        <com.google.android.gms.maps.MapView
            android:id="@+id/treeProfileMapView"
            android:layout_width="373dp"
            android:layout_height="188dp"
            android:layout_marginTop="84dp"
            app:layout_constraintEnd_toEndOf="@+id/villageTextView"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/textView15"
            app:layout_constraintTop_toBottomOf="@+id/villageTextView" />

        <ImageView
            android:id="@+id/UploadImageView1"
            android:layout_width="113dp"
            android:layout_height="83dp"
            android:layout_marginStart="60dp"
            android:layout_marginTop="30dp"
            android:src="@android:drawable/ic_input_add"
            app:layout_constraintStart_toStartOf="@+id/treeProfileMapView"
            app:layout_constraintTop_toBottomOf="@+id/treeProfileMapView" />

        <ImageView
            android:id="@+id/UploadImageView2"
            android:layout_width="119dp"
            android:layout_height="80dp"
            android:layout_marginStart="36dp"
            android:src="@android:drawable/ic_input_add"
            app:layout_constraintBottom_toBottomOf="@+id/UploadImageView1"
            app:layout_constraintStart_toEndOf="@+id/UploadImageView1"
            app:layout_constraintTop_toTopOf="@+id/UploadImageView1"
            app:layout_constraintVertical_bias="1.0" />

        <ImageView
            android:id="@+id/UploadImageView3"
            android:layout_width="110dp"
            android:layout_height="60dp"
            android:layout_marginTop="40dp"
            android:src="@android:drawable/ic_input_add"
            app:layout_constraintEnd_toEndOf="@+id/UploadImageView2"
            app:layout_constraintHorizontal_bias="0.509"
            app:layout_constraintStart_toStartOf="@+id/UploadImageView1"
            app:layout_constraintTop_toBottomOf="@+id/UploadImageView2" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/status"
            android:textColor="#000000"
            android:textSize="24sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/textView15"
            app:layout_constraintTop_toBottomOf="@+id/textView15" />


    </androidx.constraintlayout.widget.ConstraintLayout>




</ScrollView>
